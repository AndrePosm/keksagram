{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true,
    "browser" :true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    // "quotes": [2, "double", "avoid-escape"]
      "require-jsdoc": "off",
      "max-len": [2, {
        "code": 120,
        "tabWidth": 2,
        "ignoreUrls": true

      }]
  }
}
